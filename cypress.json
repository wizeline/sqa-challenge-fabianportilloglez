{
    "baseUrl": "https://todoist.com/",
    "testFiles": "**/*.feature",
    "viewportHeight": 768,
    "viewportWidth": 1024,
    "video": false,
    "reporter": "cypress-mochawesome-reporter",
    "reporterOptions": {
        "reporterEnabled": "mochawesome",
        "mochawesomeReporterOptions": {
            "reportDir": "cypress/reports/mocha",
            "quite": true,
            "overwrite": false,
            "html": false,
            "json": true
        }
    }
}