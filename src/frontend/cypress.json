{
    "baseUrl": "https://todoist.com/",
    "fixturesFolder": "cypress/fixtures",
    "reportsFolder": "./src/frontend/cypress/reports",
    "screenshotsFolder": "src/frontend/cypress/reports/screenshots",
    "integrationFolder": "src/frontend/cypress/integration/",
    "pluginsFile": "src/frontend/cypress/plugins/",
    "testFiles": "**/*.feature",
    "viewportHeight": 768,
    "viewportWidth": 1024,
    "video": false,
    "screenshotOnRunFailure": true,
    "reporter": "cypress-mochawesome-reporter",
    "reporterOptions": {
            "reportDir": "./src/frontend/cypress/reports",
            "charts": true,   
            "reportPageTitle": "Test todoist SQA Challenge",
            "reportFilename": "report-results",
            "embeddedScreenshots": true
        }
    }