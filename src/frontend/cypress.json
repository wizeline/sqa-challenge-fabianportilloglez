{
    "baseUrl": "https://todoist.com/",
    "testFiles": "**/*.feature",
    "viewportHeight": 768,
    "viewportWidth": 1024,
    "video": false,
    "screenshotOnRunFailure": true,
    "reporter": "cypress-multi-reports",
    "reporterOptions": {
        "reporterEnabled": "mochawesome",
        "mochawesomeReporterOptions": {
            "reportDir": "cypress/report/mocha",
            "reportPageTitle": "Test todoist SQA Challenge",
            "reportFilename": "report-results",
            "overwrite": true,
            "html": true,
            "json": false,
            "timestamp": "mmddyyyy_HHMMss"
        }
    }
}