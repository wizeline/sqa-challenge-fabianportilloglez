{
        "env": {
        "commonjs": true,
        "es2021": true,
        "mocha": true,
        "node": true,
        "shared-node-browser": true
    },
    "extends": [
        ["airbnb-base", 1],
        "plugin:cypress/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        } 
    },
    "rules": {
        "indent": [
        "warn",
        4
    ],
    "linebreak-style": [
        "warn",
        "unix"
    ],
    "quotes": [
        "warn",
        "single"
    ],
    "semi": [
        "warn",
        "never"
    ]
    }
}