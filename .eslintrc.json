{
        "env": {
        "commonjs": true,
        "es2021": true,
        "mocha": true,
        "node": true,
        "shared-node-browser": true
    },
    "extends": [
        "eslint:recommended",
        "prettier",
        "eslint-config-prettier",
        "plugin:cypress/recommended"
    ],
    "plugins": [
        "eslint-plugin-prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        } 
    },
    "rules": {
    "prettier/prettier": "warn",
    "linebreak-style": [
        "warn",
        "unix"
    ],
    "quotes": [
        "warn",
        "single"
    ],
    "semi": [
        "warn",
        "never"
    ]
    }
}